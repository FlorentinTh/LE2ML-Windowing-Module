  "description": "Windowing module for the LE2ML project",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .js,.json",
    "clean": "rimraf dist",
    "compile": "babel src --out-dir dist",
    "build": "npm-run-all clean compile copy-env",
    "copy-env": "cpy .env dist",
    "start": "npm-run-all build dev",
    "dev": "cross-env NODE_ENV=development node dist/index.js",
    "prod": "cross-env NODE_ENV=production node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FlorentinTh/LE2ML-Windowing-Module.git"
  },
  "author": "FlorentinTh",
  "license": "Apache-2.0",
  "pre-commit": [
    "lint"
  ],
  "bugs": {
    "url": "https://github.com/FlorentinTh/LE2ML-Windowing-Module/issues"
  },
  "homepage": "https://github.com/FlorentinTh/LE2ML-Windowing-Module#README",
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.12.1",
    "cross-env": "^7.0.3",
    "dotenv": "^9.0.2",
    "joi": "^17.4.0",
    "line-by-line": "^0.1.6",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/preset-env": "^7.14.1",
    "cpy-cli": "^3.1.1",
    "eslint": "^7.26.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2"
  }
